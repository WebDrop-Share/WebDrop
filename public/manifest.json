{
    "name": "WebDrop",
    "short_name": "WebDrop",
    "icons": [
        {
            "src": "images/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "images/android-chrome-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "images/android-chrome-192x192-maskable.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "maskable"
        },
        {
            "src": "images/android-chrome-512x512-maskable.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "maskable"
        }
    ],
    "background_color": "#efefef",
    "start_url": "/",
    "scope": "/",
    "display": "minimal-ui",
    "theme_color": "#3367d6",
    "share_target": {
        "action": "/",
        "method":"POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url",
            "files": [{
                "name": "allfiles",
                "accept": ["*/*"]
            }]
        }
    },
    "launch_handler": {
        "client_mode": "focus-existing"
    }
}
